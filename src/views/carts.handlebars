<div class="cartTitlePage">
  <h1>Carrito de Compras</h1>
</div>

<div class="cartProductsContainer">
  {{#each cart}}
    <div class="cartProduct">
      <div class="cartSection">
        <img class="cartProductImg" src="{{thumbnails}}" alt="{{title}}" />
        <h2 class="cartProductTitle">Nombre: {{title}}</h2>
      </div>
      <div class="cartSection">
        <h2 class="cartProductPrice">Precio: ${{price}}</h2>
        <h2 class="cartProductQuantity">Cantidad: {{quantity}}</h2>
      </div>
    </div>
  {{/each}}
</div>